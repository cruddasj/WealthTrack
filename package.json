{
  "name": "pwa-starter-template",
  "version": "1.0.0",
  "description": "A progressive web app starter that provides a sidebar layout, theming controls, and changelog scaffolding.",
  "main": "service-worker.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:css": "tailwindcss -i ./src/styles.css -o ./assets/styles.css --minify",
    "watch:css": "tailwindcss -i ./src/styles.css -o ./assets/styles.css --watch",
    "build:icons": "node ./scripts/generate-icons.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/example/pwa-starter-template.git"
  },
  "keywords": [],
  "author": "James Cruddas",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/example/pwa-starter-template/issues"
  },
  "homepage": "https://github.com/example/pwa-starter-template#readme",
  "devDependencies": {
    "sharp": "^0.33.4",
    "tailwindcss": "^3.4.14"
  }
}

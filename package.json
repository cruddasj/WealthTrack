{
  "name": "wealthtrack",
  "version": "1.1.106",
  "description": "WealthTrack is a personal wealth projection and planning tool built as a single-page application. It helps you capture assets and goals, run forecasts, explore portfolio insights, and maintain historical snapshots.",
  "main": "service-worker.js",
  "scripts": {
    "test": "jest",
    "test:e2e": "playwright test",
    "lint": "npm run lint:js && npm run lint:md",
    "lint:js": "eslint .",
    "lint:md": "markdownlint-cli2",
    "build:css": "tailwindcss -i ./src/styles.css -o ./assets/styles.css --minify",
    "watch:css": "tailwindcss -i ./src/styles.css -o ./assets/styles.css --watch",
    "build:icons": "node ./scripts/generate-icons.mjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cruddasj/WealthTrack.git"
  },
  "keywords": [],
  "author": "James Cruddas",
  "type": "commonjs",
  "bugs": {
    "url": "https://github.com/cruddasj/WealthTrack/issues"
  },
  "homepage": "https://github.com/cruddasj/WealthTrack#readme",
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@tailwindcss/cli": "^4.2.1",
    "@types/node": "^25.3.3",
    "eslint": "^10.0.2",
    "eslint-plugin-unused-imports": "^4.4.1",
    "eslint-plugin-yml": "^3.3.0",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "markdownlint-cli2": "^0.21.0",
    "sharp": "^0.34.5",
    "tailwindcss": "^4.2.1",
    "yaml-eslint-parser": "^2.0.0"
  },
  "engines": {
    "node": ">=20",
    "npm": ">=10"
  }
}
